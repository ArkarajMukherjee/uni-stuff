library(ggplot2)
library(dplyr)
library(scales)
# 1. Prepare data for plotting
# (Assuming you have run the 'while loop' to split midnight-crossing rows)
SZL_plot <- SZL %>%
mutate(
Date = as.Date(Start),
# Calculate fractional hours for the Y-axis (e.g., 14:30 becomes 14.5)
Start_Time = as.POSIXlt(Start)$hour + as.POSIXlt(Start)$min/60 + as.POSIXlt(Start)$sec/3600,
End_Time   = as.POSIXlt(End)$hour   + as.POSIXlt(End)$min/60   + as.POSIXlt(End)$sec/3600
)
# 2. Create the Timeline Chart
ggplot(SZL_plot) +
# Draw a rectangle for each meeting
# xmin/xmax defines the width of the day "column"
# ymin/ymax defines the start and end of the meeting
geom_rect(aes(xmin = Date - 0.4, xmax = Date + 0.4,
ymin = Start_Time, ymax = End_Time,
fill = Topic),
alpha = 0.8) +
# X-Axis: Full resolution (daily ticks) with Monthly labels
scale_x_date(
date_breaks = "1 month",
date_minor_breaks = "1 day",
labels = date_format("%b %Y")
) +
# Y-Axis: 0 to 24 hours, labeled every hour
scale_y_continuous(
breaks = 0:24,
labels = function(x) paste0(sprintf("%02d", as.integer(x)), ":00"),
limits = c(0, 24)
) +
labs(
title = "Zoom Meeting Schedule (Daily Timeline)",
subtitle = "Bars represent the actual time span of meetings",
x = "Date",
y = "Time of Day (24h)",
fill = "Topic"
) +
theme_minimal() +
theme(
axis.text.x = element_text(angle = 45, hjust = 1),
panel.grid.minor.x = element_line(color = "grey90") # Shows the daily resolution
)
